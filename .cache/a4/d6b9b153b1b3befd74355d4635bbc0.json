size: 7rem;\n  font-weight: 700;\n  color: var(--white-color);\n}\n\n.projects {\n  margin-top: 50px;\n}\n\n.projects:after {\n  content: \"\";\n  display: table;\n  clear: both;\n}\n\n.projects > div {\n  display: flex;\n  height: var(--proj-height);\n  width: var(--proj-width);\n  margin-bottom: 50px;\n  border-radius: var(--card-br);\n  background-color: var(--white-color);\n  float: left;\n  opacity: 0;\n\n  transition: all 0.4s cubic-bezier(.17,.22,0,.97);\n}\n\n.projects > div:active {\n  scale: 0.98;\n  transition: all 0.4s cubic-bezier(.17,.22,0,.97);\n}\n\n.projects > div:nth-child(2n) {\n  float: right;\n}\n\n.proceed {\n  height: var(--icon-size);\n  width: var(--icon-size);\n  margin: auto;\n  margin-left: var(--icon-margin-sides);\n  margin-right: var(--icon-margin-sides);\n  border-radius: var(--icon-br);\n}\n\n.proceed > img {\n  height: 100%;\n  width: 100%;\n  scale: 0.9;\n}\n\n.infoContainer {\n  width: calc(100% - calc(var(--icon-size) + var(--icon-margin-sides)));\n}\n\n.pNameContainer {\n  margin-top: calc(var(--icon-margin-sides) + 10px);\n  margin-bottom: 5px;\n  display: flex;\n}\n\n.pNameStatus {\n  width: 70px;\n  height: 7px;\n  margin: 0 10px 0 0;\n  padding: 10px;\n  font-size: 1.15rem;\n  text-align: center;\n  line-height: 7px;\n  border-radius: 10px;\n}\n\n.InDevStatus {\n  color: var(--status-dev-outline);\n  background-color: var(--status-dev-bg);\n  outline: 2px solid var(--status-dev-outline);\n}\n\n.OutdatedStatus {\n  color: var(--status-outd-outline);\n  background-color: var(--status-outd-bg);\n  outline: 2px solid var(--status-outd-outline);\n}\n\n.pName {\n  width: 100%;\n  margin: 0;\n  font-size: 1.4rem;\n  font-weight: 700;\n}\n\n.pDesc {\n  margin-top: 2px;\n}\n\n\n@media (max-width: 1100px)  {\n  .textContainer {\n    width: 100%;\n    padding-left: 25px;\n  }\n\n  .mainText {\n    width: 100%;\n    text-align: center;\n  }\n\n  .projects {\n    display: grid;\n    justify-content: center;\n  }\n\n  .projects > div:nth-child(2n) {\n    float: left;\n  }\n}"},"lineCount":null}},"error":null,"hash":"31712501accd8bcfd3cd34104fe0246a","cacheData":{"env":{}}}